<% if user_signed_in? %>
  <%= render 'form' %>
<% end %>

<% @articles.each do |article| %>
  <h3><%= article.title %></h3>
  <h3><small><%= article.created_at.strftime('%B %d, %Y') %></small></h3>
  <p><%= article.body %></p>

  <% if user_signed_in? %>
    <%= link_to "EDIT", edit_article_path(article) %>
  <% end %>
<% end %>
